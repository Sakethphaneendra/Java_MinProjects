import java.util.Random;
public class fun {

	
	public static void main(String args[])
	{
	   	Random x = new Random();
			int number = x.nextInt(7);
          if(number == 0 || number ==7)
	        {
		          number = number+x.nextInt(3+1);
		          if(number>6)
		          {
		          		number = 1;
		          }
	        }
	    System.out.println("The Random Number : "+number);
	}
}

